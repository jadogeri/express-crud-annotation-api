				"type": "string"
			},
			"UserCreationResponse": {
				"description": "Represents a user object with database timestamps and ID.",
				"properties": {
					"_id": {
						"$ref": "#/components/schemas/mongoose.Types.ObjectId",
						"description": "The unique identifier of the user (e.g., MongoDB ObjectId).",
						"example": "60b8f4d5f8a3c3001f3e9a0e"
					},
					"name": {
						"type": "string",
						"description": "The user's name.",
						"example": "John Doe"
					},
					"email": {
						"type": "string",
						"description": "The user's email.",
						"example": "JohnDoe@gmail.com",
						"pattern": "^(.+)@(.+)$"
					},
					"age": {
						"type": "integer",
						"format": "int32",
						"description": "The user's age.",
						"example": 21,
						"minimum": 0
					},
					"createdAt": {
						"type": "string",
						"format": "date-time",
						"description": "The date and time when the user was created.",
						"example": "2024-01-01T12:00:00.000Z"
					},
					"updatedAt": {
						"type": "string",
						"format": "date-time",
						"description": "The date and time when the user was last updated.",
						"example": "2024-01-01T13:00:00.000Z"
					}
				},
				"required": [
					"_id",
					"name",
					"email",
					"age",
					"createdAt",
					"updatedAt"
				],
				"type": "object",
				"additionalProperties": false
			},
			"UserCreationBody": {
				"description": "Represents a user create body object",
				"properties": {
					"email": {
						"type": "string",
