						"example": "John Doe"
					},
					"age": {
						"type": "integer",
						"format": "int32",
						"description": "The user's age.",
						"example": 21,
						"minimum": 0
					}
				},
				"required": [
					"email",
					"name",
					"age"
				],
				"type": "object",
				"additionalProperties": false
			},
