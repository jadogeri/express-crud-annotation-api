    "inversify-express-utils": "^6.5.0",
    "mongodb": "^7.0.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
